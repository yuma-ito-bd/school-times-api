{
  "name": "schoo-times-api",
  "version": "1.0.0",
  "description": "School TimesのAPIサーバ用リポジトリ",
  "main": "app/app.js",
  "scripts": {
    "conpile": "rm -rf ./dist && tsc",
    "start": "node ./dist/server.js",
    "nodemon": "nodemon -L --config nodemon/nodemon.json",
    "build": "tsc",
    "test": "jest",
    "migrate": "sequelize db:migrate --env=development",
    "migrate:undo": "sequelize db:migrate:undo --env=development",
    "seed:all": "sequelize db:seed:all --env=development",
    "seed:undo:all": "sequelize db:seed:undo:all --env=development",
    "heroku:push": "heroku container:push web -a school-times",
    "heroku:release": "heroku container:release web -a school-times"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuma-ito-bd/school-times-api.git"
  },
  "author": "Yuma Ito",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuma-ito-bd/school-times-api/issues"
  },
  "homepage": "https://github.com/yuma-ito-bd/school-times-api#readme",
  "dependencies": {
    "@types/express": "^4.17.2",
    "date-fns": "^2.10.0",
    "date-fns-timezone": "^0.1.4",
    "express": "^4.17.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^26.0.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
